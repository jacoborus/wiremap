{
  "name": "@jacobo/wiremap",
  "version": "1.0.0-alpha.4",
  "license": "MIT",
  "description": "Compositional dependency injection framework.",
  "exports": "./src/wiremap.ts",
  "tasks": {
    "test": "deno test test/",
    "lint": "deno lint src demo test",
    "check": "deno check src demo test",
    "fmt": "deno fmt --check src demo test",
    "all": "deno task test && deno task check && deno task lint && deno run demo/app/app.ts && deno run demo/framework/example/example.ts"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "src/wiremap.ts",
      "src/block.ts",
      "src/unit.ts",
      "src/mock.ts",
      "src/common.ts"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  }
}
